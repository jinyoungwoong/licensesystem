    <script type="text/javascript">
        $(document).ready(function () {
            //$(".menu-link").click(function () {
            //    document.getElementById('#kt_aside_menu').querySelectorAll('.menu-link.active')[0].className = 'menu-link';
            //    $(this).addClass('active');
            //});

            $(document).ajaxError(function (event, jqxhr, settings, thrownError) {

                if (jqxhr.status == 401) {
                    alert('세션이 종료되었습니다.');
                    location.href = '/';
                }

                //if (settings.url == "ajax/missing.html") {
                //    $("div.log").text("Triggered ajaxError handler.");
                //}
            });

        });

        function OpenPasswordChangeModal() {
            $('#txtPassWord').val('');
            $('#txtChangePwd').val('');
            $('#txtChangePwdConfirm').val('');
            //
            $('#password_modal').modal('toggle');
        }

        function SetPasswordChage() {
            //필수값 Vaild
            if ($('#txtPassWord').val().trim() == '' || $('#txtChangePwd').val().trim() == '' || $('#txtChangePwdConfirm').val().trim() == '') {
                alert('패스워드를 입력하세요.');
                return false;
            }
            //패스워드 일치 확인 Vaild
            if ($('#txtChangePwd').val().trim() != $('#txtChangePwdConfirm').val().trim()) {
                alert('변경할 패스워드 정보가 일치하지 않습니다.');
                return false;
            }
            //
            $.ajax({
                type: "GET",
                url: "/UserV1/PasswordChange",
                data: {
                    pwd: $('#txtPassWord').val(),
                    changePwd: $('#txtChangePwd').val()
                },
                contentType: "application/json; charset=utf-8",
                datatype: "json",
                async: "true",
                success: function (rdata) {
                    var isSuccess = rdata.success;
                    var msg = rdata.msg;

                    alert(rdata.msg);

                    if (isSuccess) {
                        $('#password_modal').modal('hide');
                    }
                }
            });
        }
    </script>


